#include <stdio.h>
#include <stdlib.h>

int main(void) {
	float a;
	float b;
	int resultado;

	a= 3.14;
	b =5.5;

	/*
	 do{
	 	 Codigo codigo
	 }while(Condicion);
	 Se repite lo del do hasta que se cumpla la condicion
	 */
	resultado= a < b;
	printf("Resultado de a menor que b es: %d",resultado);
}
