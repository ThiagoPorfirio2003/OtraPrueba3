#include <stdio.h>
#include <stdlib.h>
#include "calculos.h"



int main(void) {
	setbuf(stdout,NULL);
	int resultado;

	resultado= sumar();

	printf("\nResultado de la suma: %d", resultado);

	return EXIT_SUCCESS;
}


int sumar(void)
{
	int retorno;
	int numeroUno;
	int numeroDos;

	printf("Ingrese un numero: ");
	scanf("%d",&numeroUno);

	printf("Ingrese otro numero: ");
	scanf("%d",&numeroDos);

	retorno = numeroUno + numeroDos;

	return retorno;
}
